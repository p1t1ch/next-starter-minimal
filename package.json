{
  "name": "next-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "next dev",
    "build": "next build",
    "serve": "next start",
    "analyze": "ANALYZE=true npm run build",
    "storybook": "start-storybook -p 8000 -s public",
    "build-storybook": "build-storybook -s public",
    "lint:ts": "eslint . --ext .ts,.tsx",
    "lint:css": "stylelint '**/*.css' --ignore-path .gitignore",
    "format": "prettier . --write --ignore-path .gitignore",
    "lint": "npm run lint:ts && npm run lint:css && npm run format",
    "type-check": "tsc",
    "test": "jest --watch --passWithNoTests",
    "test:coverage": "jest --coverage --passWithNoTests",
    "coverage": "open ./coverage/lcov-report/index.html",
    "validate": "npm run lint && npm run type-check && npm run test:coverage",
    "setup": "npm i && npm run validate",
    "prepare": "husky install"
  },
  "browserslist": [
    "last 2 Chrome major versions",
    "last 2 Firefox major versions",
    "last 2 Safari major versions",
    "last 2 Edge major versions",
    "last 2 ChromeAndroid major versions",
    "last 2 iOS major versions"
  ],
  "lint-staged": {
    "*.{tsx,ts,css}": [
      "npm run lint"
    ]
  },
  "engines": {
    "node": "^12.22.0"
  },
  "dependencies": {
    "@next/bundle-analyzer": "^12.0.1",
    "classnames": "^2.3.1",
    "focus-visible": "^5.2.0",
    "modern-normalize": "^1.1.0",
    "next": "^12.0.1",
    "postcss": "^8.3.11",
    "postcss-focus-visible": "^6.0.1",
    "postcss-nested": "^5.0.6",
    "postcss-preset-env": "^6.7.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-query": "^3.29.1"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@storybook/addon-a11y": "^6.3.12",
    "@storybook/addon-essentials": "^6.3.12",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/builder-webpack5": "^6.3.12",
    "@storybook/manager-webpack5": "^6.3.12",
    "@storybook/react": "^6.3.12",
    "@svgr/webpack": "^5.5.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/react": "^17.0.33",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "babel-jest": "^27.3.1",
    "babel-loader": "^8.2.3",
    "eslint": "^7.23.0",
    "eslint-config-next": "^12.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^25.2.2",
    "eslint-plugin-jest-dom": "^3.9.2",
    "eslint-plugin-testing-library": "^5.0.0",
    "husky": "^7.0.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.3.1",
    "jest-watch-typeahead": "^1.0.0",
    "lint-staged": "^11.2.6",
    "prettier": "^2.4.1",
    "storybook-css-modules-preset": "^1.1.1",
    "stylelint": "^14.0.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^23.0.0",
    "typescript": "^4.4.4",
    "webpack": "^5.61.0"
  }
}
